                                                            </tr>
                                                        </thead>  
                                                        <tbody>


                                                          
                                                            <% for(add_on_products of add_on_products){ %>
                                                               
                                                                <tr>
                                                                    <!-- <td  class="checkbox-column"> <input style="cursor: pointer;" value="<%= add_on_products.id %>" name="product_id" type="checkbox" class="new-checkbox checkbox-primary"> </td> -->
                                                                    <td><%= add_on_products.name %></td>
                                                                    <td><%= add_on_products.value %></td> 
                                                                    <td class="text-right"><%= add_on_products.price %></td>
                                                                    <td class="text-right"><%= add_on_products.qty %></td>
                                                                    <td class="text-right"><%= add_on_products.excess_qty %></td>
                                                                    <td class="text-right"><%= add_on_products.total_qty %></td>
                                                                   
                                                                    <td class="text-right"><%= add_on_products.total_price %></td>
                                                                </tr>
                                                                <% }  %> 
                                                                
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <% }  %> 
                                            </div>
                                       
                                            <div class="inv--total-amounts">
                                            
                                                <div class="row mt-4">
                                                    <div class="col-sm-5 col-12 order-sm-0 order-1">
                                                    </div>
                                                    <div class="col-sm-7 col-12 order-sm-1 order-0">
                                                        <div class="text-sm-right">
                                                            <div class="row">
