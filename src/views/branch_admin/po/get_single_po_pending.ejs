                                                                -moz-appearance: none;
                                                                appearance: none;
                                                                margin: 0; 
                                                            }
                                                            </style>
                                                            
                                            <div class="inv--product-table-section">
                                                <h6 class="ml-2 pb-2" style="font-weight: 900;">Subscription Products : </h6>
                                                <% if(subscription_products.length ===0){ %> 
                                                    <h6 class="ml-4">No Subscription Products Found</h6>
                                                    <% }else{ %> 
                                                <div class="table-responsive alterateTable">
                                                    <table class="table">
                                                        <thead class="">
                                                            <tr>
                                                                <!-- <th class="checkbox-column dt-no-sorting"> Choose</th> -->
                                                                <th scope="col" >Name</th>
                                                                <th scope="col">Unit</th>
                                                                <th class="text-right" scope="col">Price</th>
                                                                <th class="text-right" scope="col">Qty</th>
                                                                <th class="text-right" scope="col">Excess Qty</th>
                                                                <th class="text-right" scope="col">Total Qty</th>
                                                                <th class="text-right" scope="col">Amount</th>
                                                            </tr>
                                                        </thead>  
                                                        <tbody>

                                                            

                                                            
                                                                     <% for(sub_products of subscription_products){ %>
                                                               
                                                                <tr>
                                                                    <!-- <td  class="checkbox-column"> <input style="cursor: pointer;" value="<%= sub_products.id %>" name="product_id" type="checkbox" class="new-checkbox checkbox-primary"> </td> -->
                                                                    <td name="name"><%= sub_products.name %></td>
                                                                    <td><%= sub_products.value %></td> 
                                                                    <td class="text-right"><%= sub_products.price %></td>
                                                                    <td class="text-right"><%= sub_products.qty %></td>
                                                                    <td class="text-right"><%=sub_products.excess_qty  %> </td>
                                                                    <td class="text-right"><%=sub_products.total_qty  %> </td>
                                                                    <td class="text-right"><%= sub_products.total_price %> </td>
                                                                </tr>
                                                                <% }  %> 
                                                                
                                                                
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <% }  %> 
                                                </div>



                                            <div class="inv--product-table-section">
                                                <h6 class="ml-2 pb-2" style="font-weight: 900;">Add On Products : </h6>
                                                <% if(add_on_products.length ===0){ %> 
                                                    <h6 class="ml-4">No Add On Products Found</h6>
                                                    <% }else{ %> 
